{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": "off",
        "max-len": "off",
        "require-jsdoc": "off",
        "comma-dangle": "off",
        "camelcase": "off",
        "no-unused-vars": "off",
        "arrow-parens": "off",
        "guard-for-in": "off",
        "new-cap": "off",
        "keyword-spacing": "off"
    }
}
